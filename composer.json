{
    "name": "gryfoss/sonata-precise-int-filter",
    "description": "A Sonata Admin filter for precise integer operations with float-to-int conversion",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "GryfOSS",
            "email": "contact@gryfoss.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "sonata-project/doctrine-orm-admin-bundle": "^4.19",
        "symfony/form": "^7.3",
        "gryfoss/int-precision-helper": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "doctrine/dbal": "^3.0",
        "doctrine/orm": "^2.0",
        "symfony/framework-bundle": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "GryfOSS\\SonataAdmin\\Filter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GryfOSS\\SonataAdmin\\Filter\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    }
}
